@if (Model.GroupUserPermission.IsEditRequest > 0) {
<div class="modal fade" id="EditDateFinishModal" tabindex="-1" role="dialog" aria-labelledby="exampleStandardModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleStandardModalLabel">Modal title</h5>
